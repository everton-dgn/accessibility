{
  "name": "accessibility",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Éverton Toffanetto",
  "description": "Este projeto procura aplicar boas práticas de acessibilidade web",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src/**/*.{ts,tsx,js,jsx,json} --max-warnings=0",
    "next:lint": "next lint",
    "typecheck": "tsc --project tsconfig.json --skipLibCheck --noEmit",
    "prepare": "husky install",
    "test": "jest --runInBand --passWithNoTests",
    "test:w": "jest --watch --runInBand --passWithNoTests",
    "test:coverage": "jest --runInBand --passWithNoTests --coverage",
    "sb": "start-storybook -p 6006",
    "build-sb": "build-storybook",
    "generate": "yarn plop --plopfile componentGenerator/plopfile.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts(x)?": [
      "yarn lint && yarn next:lint && yarn typecheck",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "next": "^12.0.8",
    "next-pwa": "^5.4.4",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@next/eslint-plugin-next": "^12.0.8",
    "@storybook/addon-a11y": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.38",
    "@types/react-slick": "^0.23.8",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-next-dynamic": "^1.0.1",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^12.2.2",
    "plop": "^3.0.5",
    "prettier": "^2.5.1",
    "storybook-addon-next-router": "^3.1.1",
    "storybook-dark-mode": "^1.0.8",
    "storybook-mobile": "^1.0.0",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0"
  }
}
